{
  "name": "Maplink",
  "version": "1.0.0",
  "description": "Official theme for Maplink Developers",
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clear": "rm -rf public/*",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint \"./assets/src/scss/**/*.scss\"",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "kaywords": [
    "wordpress"
  ],
  "author": "Willian Silva",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@wordpress/eslint-plugin": "^12.6.0",
    "@wordpress/stylelint-config": "^20.0.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "file-loader": "^6.2.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "postcss-loader": "^7.0.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-order": "^5.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "svgo": "^2.8.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {}
}
